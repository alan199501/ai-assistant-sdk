{
    "name": "alan199501/ai-assistant-sdk",
    "description": "PHP SDK for AI Assistant integration with JWT authentication and conversation limits",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "ai",
        "assistant",
        "chatbot",
        "jwt",
        "sdk",
        "conversation",
        "chat",
        "widget"
    ],
    "homepage": "https://github.com/alan199501/ai-assistant-sdk",
    "authors": [
        {
            "name": "MaxWebTech",
            "email": "support@maxwebtech.com",
            "homepage": "https://maxwebtech.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "firebase/php-jwt": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MaxWebTech\\AiAssistant\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MaxWebTech\\AiAssistant\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage",
        "analyse": "phpstan analyse",
        "cs-check": "php-cs-fixer check",
        "cs-fix": "php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}